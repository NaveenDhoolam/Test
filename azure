
//DEV-1 ADDING THE SAMPLE YML PIPELINE COD
trigger:
- main

pool:
  vmImage: 'windows-latest'

//DEV-2 ADDING THE SAMPLE VARIABLES
variables:
  azureSubscription: 'YourAzureSubscriptionServiceConnection' # Replace with your service connection name
  webAppName: 'YourWebAppName' # Replace with your web app name
  resourceGroupName: 'YourResourceGroupName' # Replace with your resource group name

steps:
- task: DotNetCoreCLI@2
  displayName: 'Restore'
  inputs:
    command: 'restore'
    projects: '**/*.csproj'

- task: DotNetCoreCLI@2
  displayName: 'Build'
  inputs:
    command: 'build'
    projects: '**/*.csproj'
    arguments: '--configuration Release'

- task: DotNetCoreCLI@2
  displayName: 'Test'
  inputs:
    command: 'test'
    projects: '**/*.csproj'
    arguments: '--configuration Release'trigger:
